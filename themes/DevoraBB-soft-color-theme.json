{
  "name": "DevoraBB Soft",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "focusBorder": "#4d6aff66",
    "foreground": "#cfd8f6",
    "descriptionForeground": "#8f9abb",
    "errorForeground": "#ff6b88",
    "icon.foreground": "#9da8d7",
    "activityBar.background": "#151b2b",
    "activityBar.foreground": "#cfd8f6",
    "activityBar.inactiveForeground": "#8792bf",
